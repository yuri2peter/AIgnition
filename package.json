  "name": "aignition",
  "description": "AIgnition is an intuitive, open-source note-taking web app, powered by AI to simplify and enhance your note-taking experience.",
  "version": "0.0.1",
  "author": "yuri2peter@github",
  "type": "commonjs",
  "boilerplate": {
    "project": "booza",
    "keywords": [
      "yuri2peter",
      "boilerplate",
      "electron",
      "react"
    ],
    "version": "1.0.0",
    "homepage": "https://github.com/yuri2peter/booza",
