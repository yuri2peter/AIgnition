  "scripts": {
    "start": "npm run start:node",
    "start1": "tsx dev/npm.scripts.menu.ts",
    "start:node": "concurrently \"npm run dev:renderer\" \"npm run dev:node-server\"",
    "start:electron": "concurrently \"npm run dev:renderer\" \"npm run dev:electron-server",
    "dev:renderer": "concurrently \"cross-env NODE_ENV=development webpack serve --config ./dev/webpack.config.renderer.ts\" \"cross-env NODE_ENV=development ts-node src/renderer/startChii.ts\"",
    "dev:node-server": "nodemon --config ./dev/nodemon.node.server.json",
    "dev:electron-server": "nodemon --config ./dev/nodemon.electron.server.json",
    "build": "concurrently \"npm run build:renderer\" \"npm run build:server && npm run build:preload\"",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./dev/webpack.config.renderer.ts",
    "build:preload": "cross-env NODE_ENV=production webpack --config ./dev/webpack.config.preload.ts",
    "build:server": "cross-env NODE_ENV=production webpack --config ./dev/webpack.config.server.ts",
    "prod:node": "npm run build && node dist/server/main.js",
    "prod:electron": "npm run build && electron dist",
    "package:regular": "npm run build && ts-node package/regular/package.ts",
    "package:docker": "npm run build && ts-node package/docker/package.ts",
    "package:protable": "npm run build && ts-node package/protable/package.ts",
    "package:electron": "npm run build && ts-node package/electron/package.ts",
    "package:project": "ts-node package/project/package.ts",
    "postinstall": "patch-package",
    "free-port1": "npx kill-port 8000",
    "free-port2": "npx kill-port 3000",
    "lint": "gts lint"
  },
  "engines": {
    "node": ">=16.0.0"
  },
